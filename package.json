{
  "name": "nx-starter",
  "version": "1.0.1",
  "description": "A simple MVC starter template with node, express, sequelize and socket.io",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js"
  },
  "bin": {
    "ne-starter": "index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/khokonm/node-express-starter-2.0.git"
  },
  "_moduleAliases": {
    "@controllers": "./controllers",
    "@events": "./events",
    "@libs": "./libs",
    "@middleware": "./middleware",
    "@models": "./models",
    "@routes": "./routes",
    "@services": "./services",
    "@styles": "./styles",
    "@themes": "./themes",
    "@utils": "./utils",
    "@validations": "./validations",
    "@views": "./views"
  },
  "keywords": [
    "node",
    "express",
    "socket",
    "starter",
    "template"
  ],
  "author": "Khokon M. (khokon.dev)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/khokonm/node-express-starter-2.0/issues"
  },
  "homepage": "https://github.com/khokonm/node-express-starter-2.0#readme",
  "dependencies": {
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "express": "^4.21.0",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.16",
    "nodemon": "3.1.7",
    "sequelize": "^6.32.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14"
  }
}
